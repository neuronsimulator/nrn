objref rt, z
z = new List("NMODLRandom")
proc assert() {
  if ($1 == 0) {
    hoc_execerror("assert", "")
  }
}

// ARTIFICIAL_CELL syntax tests (same as POINT_PROCESS wrt RANDOM)
rt = new RanTst()

print rt.ran1
assert(z.count == 0)

assert(rt.ran1.set_seq(5).set_ids(7,8,9).get_seq() == 5)

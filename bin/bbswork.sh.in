#!/bin/sh

prefix=@prefix@
exec_prefix=@exec_prefix@
NRNBIN=@bindir@
ARCH=@host_cpu@
MODSUBDIR=@modsubdir@
NEURONHOME=@prefix@/share/nrn

cd $1

if [ -x ${MODSUBDIR}/special ] ; then
	program="./${MODSUBDIR}/special"
else
	program="${NRNBIN}/nrniv"
fi

hostname
pwd
shift
shift
echo "time $program $*"
time $program $*

